<style>
/* You could choose to override all browser styles on all your inputs, but I prefer explicit classes */
body {
  background: #fff;
  font-size: 14px;
  font-family: Helvetica, Arial, sans-serif;
  text-rendering: optimizeLegibility;
  color: #444;
}


/* GET RID OF LABELS */

.value label {
  display: none;
}


/* STYLE LABEL TD  */

td.label {
  font-weight: 700;
  padding-right: 1em;
}


/* STYLE VALUE RADIO TD */

.value::before {
  /* content: 'Colour'; */
  display: block;
  float: left;
  margin: 10px 15px 0 0;
}


/* RADIO */

.value input[type="radio"] {
  position: relative;
  width: 26px;
  height: 26px;
  cursor: pointer;

}


/* HIDE THE FIRST RADIO GENERATED FROM SELECT OPTIONS */

.value input[type="radio"]:first-child {
  display: none;
}


/* TOP COLOR BLOCK */

input[type="radio"]:before {

  position: absolute;
  content: "";
  top: 0px;
  left: 0px;
  width: 24px;
  height: 24px;
  border: 1px solid #dddddd;
  z-index: 2;
}


/* BG COLOR BLOCK */

input[type="radio"]:after {
  border: 1px solid #dddddd;
  background: #fff;
  position: absolute;
  content: "";
  width: 24px;
  height: 24px;
  z-index: 1;
}


/* HOVER */

input[type="radio"]:hover:after {
  /* border-color: #000; */
  background: black;
}


/* CHECKED STATES */

.value input[type="radio"]:checked:after {
  //border: 1px solid #000;
  content: '';
  display: block;
  top: 2px;
  left: 2px;

  width: 20px;
  height: 20px;
  background-color: black;
  z-index: 3;
  }


.value input[type="radio"]:checked:hover:after {
  background: black;
}


/* COLORED ATTRIBUTES */

.value input[type="radio"][value="black"]:before {
  
}

.value input[type="radio"][value="white"]:before {
 background: white;
}

.value input[type="radio"][value="red"]:before {
  background: red;
}
.value input[type="radio"][value="blue"]:before {
  background: blue;
}


